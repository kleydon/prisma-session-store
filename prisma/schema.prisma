// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
}

generator client {
  provider = "prisma-client"
  output   = "../prisma/generated"
}

model Session {
  id      String   @id
  sid     String   @unique
  data    String
  expiresAt DateTime
}

model OtherSession {
  id      String   @id
  sid     String   @unique
  data    String
  expiresAt DateTime
}